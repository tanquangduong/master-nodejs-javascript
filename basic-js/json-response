var messages = [
    "Hello World",
    "Hello Serverless",
    "I learn something new today",
    "Do what I love",
    "I have a dream",
    "Hello Paris",
    "I love AWS",
    "Dream bigger",
    "Speak less, think more",
    "I love coding"
];

let idx_rand = Math.floor(Math.random()*10)
let message = messages[idx_rand]

const response = {
    statusCode: 200,
    body: JSON.stringify(message),
};


console.log(idx_rand)
console.log(response)
console.log(response.body)
console.log(response.statusCode)